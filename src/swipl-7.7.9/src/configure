/* end confdefs.h.  */

#include <execinfo.h>
#include <stdio.h>
#include <stdlib.h>

#define BT_MAX_DEPTH 10

int
print_backtrace(void)
{ void *bt[BT_MAX_DEPTH];
  char **strings;
  int i, d;

  d = backtrace(bt, BT_MAX_DEPTH);
  if ( d == -1 )
    return 1;

  strings = backtrace_symbols(bt, d);

  if (strings == NULL)
    return 1;

  free(strings);

  return 0;
}

int
fac(int n)
{ if ( n <= 1 )
  { if ( print_backtrace() )
      exit(1);
    return n;
  }
  int m = fac(n-1)*n;
  return m;
}

int
main(int argc, char**argv)
{ fac(10);

  return 0;
}

