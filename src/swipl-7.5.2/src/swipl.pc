prefix=/usr/local
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include

PLBASE=/usr/local/swipl-7.5.2
PLARCH=i386-win32
INSTALL_PLARCH=
PLLIBS=-limagehlp -ldbghelp -lm -lpthread -lwsock32 -lwinmm -lshell32 
PLLIB=-lswipl
PLCFLAGS= -D__WINDOWS__ -DWIN32 
PLLDFLAGS=
PLSOEXT=dll

Name: swipl
Description: ISO/Edinburgh-style Prolog compiler
Version: 7.5.2
Libs: ${PLLDFLAGS}  -L/usr/local/swipl-7.5.2/lib/ -lswipl
Cflags: ${PLCFLAGS} -I/usr/local/swipl-7.5.2/include
